<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
	 "http://castor.org/mapping.dtd">
<mapping>
	<description>XML的映射</description>

	<class name="com.dahuangit.seobi.receiver.dto.oxm.request.QQTalkContentXml"
		auto-complete="true">
		<field name="qqTalkImgXmls" type="com.dahuangit.seobi.receiver.dto.oxm.request.QQTalkImgXml"
			collection="arraylist">
			<bind-xml name="talk-img" />
		</field>
	</class>

	<class name="com.dahuangit.seobi.receiver.dto.oxm.request.QQTalkMsgXml"
		auto-complete="true">
		<field name="qqTalkContentXmls"
			type="com.dahuangit.seobi.receiver.dto.oxm.request.QQTalkContentXml"
			collection="arraylist">
			<bind-xml name="talk-content" />
		</field>
	</class>

	<class name="com.dahuangit.seobi.receiver.dto.oxm.request.QQTalkMsgsReqXml">
		<map-to xml="qq-talk-msgs-req" />
		<field name="clientpwd" />
		<field name="qqTalkMsgXmls" type="com.dahuangit.seobi.receiver.dto.oxm.request.QQTalkMsgXml"
			collection="arraylist">
			<bind-xml name="qq-talk-msg" />
		</field>
	</class>

</mapping>
